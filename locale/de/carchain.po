#: ../app/controllers/users/roles_controller.rb:18
msgid "Role may not be empty!"
msgstr "Die Rolle darf kein leerer Text sein!"

#: ../app/controllers/users/roles_controller.rb:23
msgid "User #%{user} already has role %{role}."
msgstr "Nutzer #%{user} hat bereits die Rolle %{role}."

#: ../app/controllers/users/roles_controller.rb:25
msgid "Role %{role} added to user #%{user}"
msgstr "Die Rolle %{role} wurde zu dem Nutzer #%{user} hinzugefügt."

#: ../app/controllers/users/roles_controller.rb:27
msgid "Could not add role %{role} to user #%{user}"
msgstr "Die Rolle %{role} konnte nicht zu dem Nutzer #%{user} hinzugefügt werden."

#: ../app/controllers/users/roles_controller.rb:41
msgid "You can't remove that role, because the user could no longer manage roles."
msgstr "Du kannst diese Rolle nicht entfernen, da der Nutzer dann die Berechtigung zum Verwalten von Rollen verliert."

#: ../app/controllers/users/roles_controller.rb:43
msgid "Role %{role} removed from user #%{user}"
msgstr "Die Rolle %{role} wurde von dem Nutzer #%{user} entfernt."

#: ../app/controllers/users/roles_controller.rb:46
msgid "Could not remove role %{role} from user #%{user}"
msgstr "Die Rolle %{role} konnte nicht von dem Nutzer #%{user} entfernt werden."

